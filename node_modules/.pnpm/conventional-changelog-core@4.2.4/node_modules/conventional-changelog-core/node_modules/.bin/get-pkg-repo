#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/src/node_modules:/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/node_modules:/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules:/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/src/node_modules:/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules/get-pkg-repo/node_modules:/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/get-pkg-repo@4.2.1/node_modules:/mnt/k/Project/FLM/Version 1.79/FLM Version 1.79/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../get-pkg-repo@4.2.1/node_modules/get-pkg-repo/src/cli.js" "$@"
else
  exec node  "$basedir/../../../../../get-pkg-repo@4.2.1/node_modules/get-pkg-repo/src/cli.js" "$@"
fi
